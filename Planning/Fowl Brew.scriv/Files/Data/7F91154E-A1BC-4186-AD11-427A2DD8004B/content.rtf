\par\plain \f0\fs24\b0\i0
\par\plain {\f0\fs24\b0\i0 Vermont}
\par\plain {\f0\fs24\b0\i0 Hi Kale.}
\par\plain \f0\fs24\b0\i0
\par\plain {\f0\fs24\b0\i0 Kale}
\par\plain {\f0\fs24\b0\i0 \loch\af0\hich\af0\dbch\af0\uc1\u8230\'85Vermont }
\par\plain \f0\fs24\b0\i0
